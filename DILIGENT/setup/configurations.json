{
  "backend": {
    "title": "DILIGENT Clinical Copilot Backend",
    "version": "0.1.0",
    "description": "FastAPI backend"
  },
  "ui_runtime": {
    "host": "0.0.0.0",
    "port": 7861,
    "title": "DILIGENT Clinical Copilot",
    "mount_path": "/ui",
    "redirect_path": "/ui",
    "show_welcome_message": false,
    "reconnect_timeout": 3600.0
  },
  "api": {
    "base_url": "http://127.0.0.1:8000"
  },
  "http": {
    "timeout": 3600.0
  },
  "database": {
    "selected_database": "sqlite",
    "insert_batch_size": 1000
  },
  "client_runtime_defaults": {
    "parsing_model": "qwen3:1.7b",
    "clinical_model": "gpt-oss:20b",
    "llm_provider": "openai",
    "cloud_model": "gpt-4o-mini",
    "use_cloud_services": false,
    "ollama_temperature": 0.7,
    "ollama_reasoning": false
  },
  "ollama_host_default": "http://localhost:11434",
  "rag": {
    "vector_collection_name": "documents",
    "chunk_size": 1024,
    "chunk_overlap": 128,
    "top_k_documents": 6,
    "embedding_backend": "ollama",
    "ollama_base_url": "http://localhost:11434",
    "ollama_embedding_model": "nomic-embed-text:latest",
    "vector_index_metric": "cosine",
    "vector_index_type": "IVF_FLAT",
    "reset_vector_collection": true,
    "cloud_provider": "",
    "cloud_model": "",
    "cloud_embedding_model": "",
    "use_cloud_embeddings": false
  },
  "external_data": {
    "default_llm_timeout": 3600.0,
    "livertox_llm_timeout": 3600.0,
    "livertox_archive": "livertox_NBK547852.tar.gz",
    "livertox_yield_interval": 25,
    "livertox_skip_deterministic_ratio": 0.8,
    "livertox_monograph_max_workers": 4,
    "max_excerpt_length": 8000,
    "llm_null_match_names": [
      "",
      "none",
      "no match",
      "no matches",
      "not found",
      "unknown",
      "not applicable",
      "n a"
    ]
  },
  "drugs_matcher": {
    "direct_confidence": 1.0,
    "master_confidence": 0.92,
    "synonym_confidence": 0.9,
    "partial_confidence": 0.86,
    "fuzzy_confidence": 0.84,
    "fuzzy_threshold": 0.85,
    "token_max_frequency": 3,
    "min_confidence": 0.4,
    "catalog_excluded_term_suffixes": [
      "PCK"
    ]
  } 
  
}
